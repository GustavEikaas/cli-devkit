{
  "name": "cli-devkit",
  "version": "0.2.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "CLI devkit for making JS/TS cli tools",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "first-time-setup": "npm i -g pnpm@8.0.0 typescript && pnpm install && pnpm build",
    "build": "tsc -b -f",
    "prepublish": "pnpm bundle",
    "bundle": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "packageManager": "pnpm@8.0.0",
  "sideEffects": false,
  "keywords": [
    "cli"
  ],
  "author": "Gustav Eikaas",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.5.3",
    "@wessberg/rollup-plugin-ts": "^2.0.4",
    "pkgroll": "^2.0.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.2"
  }
}